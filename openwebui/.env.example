# Copy this file to .env and fill in your values
# Docker Compose automatically reads .env from this directory

# Archestra LLM Proxy URL (include the profile/agent UUID)
# Example: http://host.docker.internal:9000/v1/openai/<profile-id>
OPENAI_API_BASE_URL=

# OpenAI API key (forwarded by Archestra's LLM proxy to the upstream provider)
OPENAI_API_KEY=
